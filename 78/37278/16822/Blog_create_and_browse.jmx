          <stringProp name="BeanShellSampler.query">// Variables access
import org.apache.jmeter.threads.JMeterContextService;
import org.apache.jmeter.threads.JMeterContext;
import org.apache.jmeter.threads.JMeterVariables;

// &lt;insert-in-all-BeanShell-samplers&gt;
// Add JMeter classes
addClassPath( &quot;${JMETER_HOME}/lib&quot; ) ;
addClassPath( &quot;${JMETER_HOME}/lib/ext&quot; ) ;



// Get context and variables

JMeterContext jmctx = JMeterContextService.getContext();
JMeterVariables vars = jmctx.getVariables() ;

// JMeterVariables javadoc: http://jakarta.apache.org/jmeter/api/org/apache/jmeter/threads/JMeterVariables.html
//              vars.put(&quot;Key&quot;,&quot;StringValue&quot;) ;
//              vars.putObject(&quot;AnotherKey&quot;, new Long(10) ) ; // Do not cast it when retrieve :)

// Now all samplers in this Thread Group have access to variables:
//              Long k = vars.getObject(&quot;AnotherKey&quot;) ;
//              String v = vars.get(&quot;Key&quot;) ;

String str = vars.get(&quot;JMETER_HOME&quot;);
print(&quot;JMETER_HOME = &quot;+str);

str = vars.get(&quot;BEAN_SHELL_SCRIPT_HOME&quot;);
print(&quot;BEAN_SHELL_SCRIPT_HOME = &quot;+str);

str = vars.get(&quot;LOGS_PATH&quot;);
print(&quot;LOGS_PATH = &quot;+str);

str = vars.get(&quot;TEST_PLAN_HOME&quot;);
print(&quot;TEST_PLAN_HOME = &quot;+str);

print(&quot;==========================&quot;);
print(&quot;==========================&quot;);

return (&quot;===== WORKING ====&quot;);
 
    </stringProp>
