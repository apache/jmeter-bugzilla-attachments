                  <boolProp name="resetInterpreter">true</boolProp>
                  <stringProp name="script">import java.util.Random;

int count_patterns = Integer.parseInt(vars.get(&quot;community_free_patterns_count&quot;));
selected_patterns = vars.getObject(&quot;selected_patterns_for_library_items&quot;);

if (null == selected_patterns) {
	selected_patterns = new HashMap();
}

int selected_index = 1 + (int) (Math.random() * (count_patterns - 1));
while (selected_patterns.containsKey(selected_index)) {
	selected_index = 1 + (int) (Math.random() * (count_patterns - 1));
}

vars.put(&quot;selected_free_pattern_id&quot;, vars.get(&quot;community_free_pattern_id_&quot; + selected_index.toString()));
selected_patterns.put(selected_index, vars.get(&quot;community_free_pattern_id_&quot; + selected_index.toString()));
	
vars.putObject(&quot;selected_patterns_for_library_items&quot;, selected_patterns);
