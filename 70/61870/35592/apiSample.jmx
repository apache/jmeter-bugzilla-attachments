              <stringProp name="filename"></stringProp>
              <stringProp name="script">import java.util.Random;

int count_patterns = Integer.parseInt(vars.get(&quot;community_free_patterns_count&quot;));
selected_patterns = new HashMap();
vars.putObject(&quot;community_count_library_item_created&quot;, 0);
vars.putObject(&quot;community_count_library_item_to_create&quot;, 0);
vars.put(&quot;community_create_library_item_finished&quot;, &quot;true&quot;);

//log.info(
//	&quot;INITIALIZE -- We are at &quot;
//	+ vars.getObject(&quot;community_count_library_item_created&quot;).toString()
//	+ &quot; created for customer(&quot;
//	+ vars.get(&quot;community_customer_id&quot;)
//	+ &quot;).&quot;
//	);
	
if (count_patterns &lt; 1) {
	log.info(
		&quot;We won&apos;t create library items for Customer(&quot;
		+ vars.get(&quot;community_customer_id&quot;)
		+ &quot;).&quot;
	);

	return ;
}

int nb_created = 1 + (int) (Math.random() * (count_patterns - 1));

vars.putObject(&quot;community_count_library_item_to_create&quot;, nb_created);
vars.put(&quot;community_create_library_item_finished&quot;, &quot;false&quot;);

//log.info(
//	&quot;We will create &quot;
//	+ nb_created.toString()
//	+ &quot; library items for Customer(&quot;
//	+ vars.get(&quot;community_customer_id&quot;)
//	+ &quot;).&quot;
//);
</stringProp>
