        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="JSR223 Sampler" enabled="true">
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters">${__Random(1,100000000,)}</stringProp>
          <stringProp name="script">import org.apache.kafka.clients.producer.KafkaProducer;
import org.apache.kafka.clients.producer.ProducerRecord;
KafkaProducer&lt;Integer, String&gt; producer = null;
try {
	//Set connection properties
	Properties props = new Properties();
	props.put(&quot;bootstrap.servers&quot;, &quot;localhost:9092&quot;);
	props.put(&quot;key.serializer&quot;, &quot;org.apache.kafka.common.serialization.IntegerSerializer&quot;);
	props.put(&quot;value.serializer&quot;, &quot;org.apache.kafka.common.serialization.StringSerializer&quot;);
	producer = new KafkaProducer&lt;Integer, String&gt;(props);

	//Set Data
	ProducerRecord&lt;Integer, String&gt; producerRecord = new ProducerRecord&lt;Integer, String&gt;(&quot;test&quot;, 49, &quot;value&quot;+Parameters);
	producer.send(producerRecord);
	SampleResult.setSuccessful(true);

} catch (Exception e) {
	log.error(&quot;&quot; + e);
	//e.printStackTrace();
	SampleResult.setSuccessful(false);
} finally {
	if (producer != null) {
		try {
			//Close
			producer.close();
		} catch (Exception ex) {
			// NOOP
		}
	}
}
</stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
        </JSR223Sampler>
